    <validationRules>
        <fullName>End_date_must_be_1_day_ahead_of_Start</fullName>
        <active>true</active>
        <description>The Event End Date and Time must be 1 day apart from the Event Start Date and Time</description>
        <errorConditionFormula>AND( DATEVALUE(Event_End_Date_Time__c)&lt; (DATEVALUE(Event_Start_Date_Time__c)+1), NOT(ISNULL(Event_End_Date_Time__c)))</errorConditionFormula>
        <errorDisplayField>Event_End_Date_Time__c</errorDisplayField>
        <errorMessage>End Date and Start Date must be 1 day apart</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>In_Person_Event</fullName>
        <active>true</active>
        <description>Attendees must select a location for In-Person events</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;In-person&apos;) ,   (ISBLANK(Location_List__c ) ) )</errorConditionFormula>
        <errorDisplayField>Location_List__c</errorDisplayField>
        <errorMessage>Please select a location for In-Person Events</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Requirements</fullName>
        <active>true</active>
        <description>Frequency field requirement</description>
        <errorConditionFormula>OR(AND(Recurring__c = TRUE, ISBLANK( TEXT( Frequency__c ))),AND(Recurring__c  = FALSE , NOT(ISBLANK( TEXT( Frequency__c )))))</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>This is a recurring event. Please update the event frequency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Virtual_Event</fullName>
        <active>true</active>
        <description>If the event is virtual, attendees cannot select a location</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;Virtual&apos;) ,   NOT(ISBLANK(Location_List__c ) ) )</errorConditionFormula>
        <errorDisplayField>Location_List__c</errorDisplayField>
        <errorMessage>Event is Virtual</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
